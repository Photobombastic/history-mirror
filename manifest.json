{
  "manifest_version": 3,
  "name": "HistoryMirror",
  "version": "1.3",
  "permissions": ["scripting", "tabs", "activeTab"],
  "host_permissions": ["http://*/*", "https://*/*"],
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": ["contentScript.js", "modal.html", "modal.css"],
      "matches": ["<all_urls>"]
    }
  ]
}
